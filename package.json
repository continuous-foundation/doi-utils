{
  "name": "doi-utils",
  "version": "0.0.1",
  "description": "Set of utility functions to help with handling DOI(Digital Object Identifier)",
  "main": "dist/index.js",
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build_types": "tsc -b .",
    "build_esm": "swc -C module.type=es6 -d dist src/",
    "clean": "rm -rf dist",
    "build": "npm run clean && npm run build_types && npm run build_esm",
    "test": "jest"
  },
  "keywords": [
    "doi",
    "tookit"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.186",
    "@swc/jest": "^0.2.21",
    "@types/jest": "^27.5.1",
    "jest": "^27.1.0",
    "typescript": "^4.6.4"
  }
}
