{
  "name": "doikit",
  "version": "0.0.1",
  "description": "A toolkit to for handling DOI(Digital Object Identifier)",
  "main": "dist/index.js",
  "type": "module",
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "scripts": {
    "build_types": "tsc -b .",
    "build_esm": "swc -C module.type=es6 -d dist src/",
    "build": "npm run build_types && npm run build_esm",
    "test": "jest"
  },
  "keywords": [
    "doi",
    "tookit"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.186",
    "@swc/jest": "^0.2.21",
    "@types/jest": "^27.5.1",
    "jest": "^28.1.0",
    "swc": "^1.0.11",
    "typescript": "^4.6.4"
  }
}
